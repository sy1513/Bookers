<h1>Books</h1>
<table>
   <thead>
     <tr>
       <th>Title</th>
       <th>body</th>
     </tr>
   </thead>
    <tbody>
      <%=book.title %>
      <%=book.body%>
    </tbody>










<h3>New book</h3>
<!-- form_with部分 -->
<%= form_with model: @book, local:true do |f| %>
  <h4>title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'create book' %>
<% end %>